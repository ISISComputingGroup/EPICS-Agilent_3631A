record(bo, "$(P)SIM")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
}

record(bo, "$(P)DISABLE") 
{
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "0")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}
record(stringin, "$(P)MESSAGE")
{
    field(DTYP, "asynOctetRead")
    field(INP,  "@asyn(frontpanel,0,0)Message")
    field(SCAN, ".1 second")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:MESSAGE")
    field(SDIS, "$(P)DISABLE")
}

record(ai, "$(P)CURRENT")
{
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn(frontpanel,0,0)Current")
    field(PREC, "3")
    field(SCAN, ".1 second")
    field(SIML, "$(P)SIM")
    field(SIOL, "$(P)SIM:CURRENT")
    field(SDIS, "$(P)DISABLE")
	field(EGU, "A")
}

