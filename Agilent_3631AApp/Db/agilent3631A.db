record(stringin, "$(P)MESSAGE")
{
    field(DTYP, "asynOctetRead")
    field(INP,  "@asyn(frontpanel,0,0)Message")
    field(SCAN, ".1 second")
}

record(ai, "$(P)CURRENT")
{
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn(frontpanel,0,0)Current")
    field(PREC, "3")
    field(SCAN, ".1 second")
}

record(ao, "$(P)CURRENT:SP")
{
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn(frontpanel,0,0)Current_Setpoint")
}

